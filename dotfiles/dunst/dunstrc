[global]
    monitor = 0
    origin = top-right
    notification_limit = 3
    indicate_hidden = yes

[urgency_low]
    background = "#16161e"
    foreground = "#c0caf5"
    frame_color = "#c0caf5"
    timeout = 5

[urgency_normal]
    background = "#1a1b26"
    foreground = "#c0caf5"
    frame_color = "#c0caf5"
    timeout = 5

[urgency_critical]
    background = "#292e42"
    foreground = "#db4b4b"
